{
  "dashboard": {
    "id": null,
    "title": "QuantDesk Trading Analytics",
    "tags": ["quantdesk", "trading", "defi", "solana"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Trading Volume (24h)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(quantdesk_trading_volume_24h)",
            "legendFormat": "Total Volume ($)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "displayMode": "basic",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10000},
                {"color": "red", "value": 50000}
              ]
            },
            "unit": "currencyUSD"
          }
        }
      },
      {
        "id": 2,
        "title": "Active Traders",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "sum(quantdesk_active_traders)",
            "legendFormat": "Active Traders",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "displayMode": "basic",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "title": "Total Value Locked",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(quantdesk_total_value_locked)",
            "legendFormat": "TVL ($)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "displayMode": "basic",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100000},
                {"color": "red", "value": 500000}
              ]
            },
            "unit": "currencyUSD"
          }
        }
      },
      {
        "id": 4,
        "title": "Open Positions",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(quantdesk_total_positions)",
            "legendFormat": "Open Positions",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "displayMode": "basic",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 200}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "title": "API Response Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "quantdesk_api_response_time",
            "legendFormat": "Response Time (ms)",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (ms)",
            "min": 0,
            "max": null,
            "unit": "ms"
          }
        ],
        "xAxis": {
          "mode": "time",
          "name": null,
          "show": true,
          "values": []
        },
        "lines": true,
        "linewidth": 1,
        "nullPointMode": "null",
        "pointradius": 2,
        "points": false,
        "renderer": "flot",
        "seriesOverrides": [],
        "spaceLength": 10,
        "stack": false,
        "steppedLine": false,
        "thresholds": [],
        "timeFrom": null,
        "timeShift": null,
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        },
        "type": "graph"
      },
      {
        "id": 6,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "quantdesk_memory_usage",
            "legendFormat": "Memory Usage (MB)",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Memory (MB)",
            "min": 0,
            "max": null,
            "unit": "MB"
          }
        ],
        "xAxis": {
          "mode": "time",
          "name": null,
          "show": true,
          "values": []
        },
        "lines": true,
        "linewidth": 1,
        "nullPointMode": "null",
        "pointradius": 2,
        "points": false,
        "renderer": "flot",
        "seriesOverrides": [],
        "spaceLength": 10,
        "stack": false,
        "steppedLine": false,
        "thresholds": [],
        "timeFrom": null,
        "timeShift": null,
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        },
        "type": "graph"
      },
      {
        "id": 7,
        "title": "Market Volume Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "quantdesk_market_volume_by_symbol",
            "legendFormat": "{{symbol}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          }
        }
      },
      {
        "id": 8,
        "title": "Leverage Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16},
        "targets": [
          {
            "expr": "quantdesk_leverage_distribution",
            "legendFormat": "{{leverage_range}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          }
        }
      },
      {
        "id": 9,
        "title": "Long/Short Ratio by Market",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16},
        "targets": [
          {
            "expr": "quantdesk_long_short_ratio",
            "legendFormat": "{{symbol}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "scaleDistribution": {
                "type": "linear"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "green", "value": 1}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 10,
        "title": "Trading Volume Over Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "rate(quantdesk_trading_volume_24h[5m])",
            "legendFormat": "Volume Rate (5m)",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Volume Rate",
            "min": 0,
            "max": null,
            "unit": "currencyUSD"
          }
        ],
        "xAxis": {
          "mode": "time",
          "name": null,
          "show": true,
          "values": []
        },
        "lines": true,
        "linewidth": 1,
        "nullPointMode": "null",
        "pointradius": 2,
        "points": false,
        "renderer": "flot",
        "seriesOverrides": [],
        "spaceLength": 10,
        "stack": false,
        "steppedLine": false,
        "thresholds": [],
        "timeFrom": null,
        "timeShift": null,
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        },
        "type": "graph"
      }
    ],
    "templating": {
      "list": [
        {
          "name": "market",
          "type": "query",
          "query": "label_values(quantdesk_market_volume_by_symbol, symbol)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "datasource": "Neon ML Trading"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Neon ML Trading",
          "enable": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "tags": ["deployment"],
          "type": "tags"
        }
      ]
    }
  }
}
