    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// QuantDesk Complete Platform - Pre-request Script",
          "console.log('üöÄ QuantDesk API Request:', pm.request.name);",
          "console.log('üìä Endpoint:', pm.request.url.toString());",
          "",
          "// Add timestamp to all requests",
          "pm.globals.set('timestamp', new Date().toISOString());",
          "",
          "// Auto-refresh auth token if needed",
          "if (pm.request.headers.has('Authorization') && pm.request.headers.get('Authorization').includes('{{AUTH_TOKEN}}')) {",
          "    const authToken = pm.environment.get('AUTH_TOKEN');",
          "    if (!authToken) {",
          "        console.log('‚ö†Ô∏è No auth token found, please authenticate first');",
          "    }",
          "}",
          "",
          "// Log request type",
          "const requestType = pm.request.url.toString().includes('/api/v1/') ? 'Data Pipeline' : 'Backend';",
          "console.log('üîß Request Type:', requestType);"
        ]
      }
    },
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// QuantDesk Complete Platform - Response Tests",
          "pm.test('Response time is acceptable', function () {",
          "    pm.expect(pm.response.responseTime).to.be.below(10000);",
          "});",
          "",
          "pm.test('Response status is successful', function () {",
          "    pm.expect(pm.response.code).to.be.oneOf([200, 201, 202]);",
          "});",
          "",
          "pm.test('Response has valid content', function () {",
          "    pm.expect(pm.response.text()).to.not.be.empty;",
          "});",
          "",
          "// Log response details",
          "console.log('‚úÖ QuantDesk Response:', pm.response.status);",
          "console.log('üìä Response Time:', pm.response.responseTime + 'ms');",
          "console.log('üìè Response Size:', pm.response.responseSize + ' bytes');",
          "",
          "// Store response data for debugging",
          "if (pm.response.json && pm.response.json()) {",
          "    pm.globals.set('last_response', JSON.stringify(pm.response.json()));",
          "}",
          "",
          "// Auto-extract tokens from auth responses",
          "if (pm.request.name.includes('Authenticate') && pm.response.code === 200) {",
          "    const response = pm.response.json();",
          "    if (response.token) {",
          "        pm.environment.set('AUTH_TOKEN', response.token);",
          "        console.log('üîë Auth token saved to environment');",
          "    }",
          "    if (response.refreshToken) {",
          "        pm.environment.set('REFRESH_TOKEN', response.refreshToken);",
          "        console.log('üîÑ Refresh token saved to environment');",
          "    }",
          "}",
          "",
          "// Performance monitoring",
          "if (pm.response.responseTime > 5000) {",
          "    console.log('‚ö†Ô∏è Slow response detected:', pm.response.responseTime + 'ms');",
          "}"
        ]
      }
    }
