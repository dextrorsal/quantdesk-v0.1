  "main": "dist/api/index.js",
  "bin": {
    "solana-defi-ai": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/api/index.ts",
    "start": "node dist/api/index.js",
    "cli": "tsx src/cli/index.ts",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts tests/**/*.ts",
    "format:check": "prettier --check src/**/*.ts tests/**/*.ts",
    "type-check": "tsc --noEmit",
    "security:audit": "npm audit",
    "security:scan": "npm audit --audit-level moderate",
    "docs:generate": "typedoc src --out docs/api",
    "docs:serve": "serve docs/api",
    "setup": "node scripts/setup/install.js",
    "deploy:staging": "node scripts/deploy/staging.js",
    "deploy:production": "node scripts/deploy/production.js",
    "monitor:health": "node scripts/monitoring/health-check.js",
    "monitor:metrics": "node scripts/monitoring/metrics.js",
    "clean": "rimraf dist coverage",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
